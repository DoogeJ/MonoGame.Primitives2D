<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>MonoGame.Primitives2D</PackageId>
    <Version>1.1.1</Version>
    <Authors>DoogeJ, John McDonald and Gary Texmo</Authors>
    <PackageLicenseExpression>Zlib</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/DoogeJ/MonoGame.Primitives2D</PackageProjectUrl>
    <Description>Easy-to-use 2D primitives</Description>
    <Copyright>Copyright 2012-2020</Copyright>
    <PackageTags>MonoGame XNA Drawing 2D SpriteBatch Primitives</PackageTags>
    <PackageIcon>monogame.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="monogame.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MonoGame.Framework.DesktopGL" Version="3.8.0.1641">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
  </ItemGroup>

</Project>
